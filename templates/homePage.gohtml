          }}
        </label><br>
        <input class="readonly_id form-input" readonly type="text" name="header-plugin-options" id="header-plugin-options"/><br><br>
        <div class="plugin-example" id="header-plugin-example"> </div>

        <button id="headerPlugin-goBack" class="simple-button btn-undo grow-shadow-hover" type="button">
          {{ or
            (index .TargetStrings "i18n-buttonGoBack")
            (index .DefaultStrings "i18n-buttonGoBack")
          }}
        </button>
        <button id="headerPlugin-submit" class="simple-button btn-create grow-shadow-hover" type="button">
          {{ or
            (index .TargetStrings "i18n-modalFirstTimeLangSubmit")
            (index .DefaultStrings "i18n-modalFirstTimeLangSubmit")
          }}
        </button>
        <datalist id="header-plugin-list"> </datalist>
      </form>
    </div>
